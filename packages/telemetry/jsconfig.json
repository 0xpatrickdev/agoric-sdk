// This file can contain .js-specific Typescript compiler config.
{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "checkJs": true,
    "noEmit": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
  },
  "include": [
    "*.js",
    "scripts",
    "src",
    "test",
  ],
}
