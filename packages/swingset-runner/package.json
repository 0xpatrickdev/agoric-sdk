{
  "name": "@agoric/swingset-runner",
  "version": "0.13.8",
  "private": true,
  "description": "Application to launch SwingSet instances for development and testing",
  "parsers": {
    "js": "mjs"
  },
  "main": "src/main.js",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "author": "Agoric",
  "license": "Apache-2.0",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:xs": "exit 0",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint-check": "yarn lint",
    "lint": "eslint '**/*.js'",
    "ci:autobench": "./autobench"
  },
  "dependencies": {
    "@agoric/assert": "^0.3.4",
    "@agoric/babel-standalone": "^7.14.3",
    "@agoric/bundle-source": "^1.4.2",
    "@agoric/ertp": "^0.11.8",
    "@agoric/eventual-send": "^0.13.20",
    "@agoric/install-ses": "^0.5.18",
    "@agoric/marshal": "^0.4.17",
    "@agoric/nat": "^4.1.0",
    "@agoric/same-structure": "^0.1.18",
    "@agoric/stat-logger": "^0.4.14",
    "@agoric/swing-store-lmdb": "^0.5.4",
    "@agoric/swing-store-simple": "^0.4.4",
    "@agoric/swingset-vat": "^0.18.4",
    "@agoric/tame-metering": "^2.0.4",
    "@agoric/zoe": "^0.17.3",
    "expose-gc": "^1.0.0",
    "n-readlines": "^1.0.1",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "esm": "agoric-labs/esm#Agoric-built"
  },
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintIgnore": [
    "bundle-*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "esm"
    ],
    "timeout": "2m"
  }
}
