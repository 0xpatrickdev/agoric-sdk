{
  "name": "@agoric/cosmic-swingset",
  "version": "0.31.4",
  "description": "Agoric's Cosmos blockchain integration",
  "parsers": {
    "js": "mjs"
  },
  "bin": {
    "ag-chain-cosmos": "./src/entrypoint.cjs"
  },
  "main": "src/chain-main.js",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:xs": "exit 0",
    "pretty-fix": "prettier --write '**/*.js'",
    "pretty-check": "prettier --check '**/*.js'",
    "lint-fix": "yarn lint:eslint --fix && yarn lint:types",
    "lint-check": "yarn lint",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc -p jsconfig.json",
    "lint:eslint": "eslint '**/*.js'"
  },
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "dependencies": {
    "@agoric/assert": "^0.3.3",
    "@agoric/babel-standalone": "^7.14.3",
    "@agoric/cosmos": "^0.26.6",
    "@agoric/dapp-svelte-wallet": "^0.10.6",
    "@agoric/import-bundle": "^0.2.18",
    "@agoric/install-ses": "^0.5.17",
    "@agoric/store": "^0.4.18",
    "@agoric/swing-store-lmdb": "^0.5.3",
    "@agoric/swingset-vat": "^0.18.2",
    "@agoric/vats": "^0.2.6",
    "@iarna/toml": "^2.2.3",
    "@opentelemetry/exporter-prometheus": "^0.16.0",
    "@opentelemetry/metrics": "^0.16.0",
    "agoric": "^0.13.6",
    "anylogger": "^0.21.0",
    "deterministic-json": "^1.0.5",
    "esm": "agoric-labs/esm#Agoric-built",
    "node-lmdb": "^0.9.4"
  },
  "devDependencies": {
    "ava": "^3.12.1"
  },
  "eslintConfig": {
    "extends": [
      "@endo"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=11.0"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "esm"
    ],
    "timeout": "20m"
  }
}
